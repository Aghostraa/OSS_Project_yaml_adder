{
    "manifest_version": 2,
    "name": "OSS Project Yaml Adder",
    "description": "A Chrome extension to add projects to the OSS database.",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "http://localhost:8080/"  
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  }
  